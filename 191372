      operation 3795724 "format"
	public explicit_return_type "void"
	nparams 1
	  param inout name "firstMessage" type class_ref 854156 // NdefMessage
	nexceptions 2
	  explicit_exception "IOException"
	  exception class_ref 854028 // FormatException
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
