      operation 3781004 "enableForegroundDispatch"
	public explicit_return_type "void"
	nparams 4
	  param inout name "activity" type class_ref 693260 // Activity
	  param inout name "intent" type class_ref 711948 // PendingIntent
	  param inout name "filters" type class_ref 687628 // IntentFilter
	  param inout name "techLists" explicit_type "String"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
