      operation 2313883 "setAttributeNode"
	public return_type class_ref 345627 // Attr
	nparams 1
	  param inout name "newAttr" type class_ref 345627 // Attr
	nexceptions 1
	  exception class_ref 344987 // DOMException
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${type} ${name}${(}${)}${const}${volatile}${throw}${abstract};
"
